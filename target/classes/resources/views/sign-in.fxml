<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignInController">
   <children>
      <TextField fx:id="login" alignment="CENTER" layoutX="175.0" layoutY="135.0" maxHeight="25.0" maxWidth="250.0" minHeight="25.0" minWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="Логин" />
      <PasswordField fx:id="password" alignment="CENTER" layoutX="175.0" layoutY="177.0" maxHeight="25.0" maxWidth="250.0" minHeight="25.0" minWidth="250.0" prefHeight="25.0" prefWidth="250.0" promptText="Пароль" />
      <Button layoutX="251.0" layoutY="217.0" maxHeight="25.0" maxWidth="100.0" minHeight="25.0" minWidth="100.0" mnemonicParsing="false" onAction="#onSubmit" prefHeight="25.0" prefWidth="100.0" text="Войти" />
      <Label fx:id="loginFailed" layoutX="204.0" layoutY="160.0" text="Логин или пароль введен неверно" textFill="RED" />
   </children>
</AnchorPane>
